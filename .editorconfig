# EditorConfig é útil para manter estilos de código consistentes para diferentes editores e IDEs
root = true

[*]
end_of_line = lf
charset = utf-8
indent_style = space
indent_size = 2
tab_width = 2
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

[*.ts]
indent_size = 2

[*.json]
indent_size = 2
